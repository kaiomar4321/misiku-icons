{
  "name": "misiku-icons",
  "version": "1.0.0",
  "private": true,
  "description": "Librería multiplataforma de íconos SVG animados",
  "scripts": {
    "build": "pnpm --filter ./packages/* run build",
    "prepare": "husky",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "rollup": "^4.0.0",
    "typescript": "^5.0.0",
    "prettier": "^3.4.2",
    "eslint": "^9.19.0"
  },
  "workspaces": [
    "packages/core",
    "packages/react-icons",
    "packages/vue-icons",
    "packages/svelte-icons",
    "packages/angular-icons"
  ]
}
